@{
}
@model IEnumerable<Day2Task.Models.CrsResult>

<h2>Trainee Results</h2>
<table class="table">
    <thead>
        <tr>
            <th>Course</th>
            <th>Grade</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                <tr>
                    <td>@item.Course.Name</td> 
                    <td>@item.Degree</td>
                </tr>
        }
    </tbody>
</table>
