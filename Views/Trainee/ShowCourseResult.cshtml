@{
}
@model List<CrsResult>
<h2>Course Results</h2>
<table>
    <tr><th>Trainee</th><th>Grade</th></tr>
    @foreach (var item in Model)
    {
            <tr>
                <td>@item.TraineeId</td>
                <td>@item.Degree</td>
            </tr>
    }
</table>
